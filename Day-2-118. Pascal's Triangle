class Solution {
    public List<List<Integer>> generate(int numRows) {
        List<List<Integer>> res=new ArrayList<>();
        for(int i=1;i<=numRows;i++)
        {
            List<Integer> temp=new ArrayList<>();
            temp.add(1);
            for(int j=1;j<=i-2;j++)
            {
                int last=res.get(i-2).get(j);
                int sLast=res.get(i-2).get(j-1);
                temp.add(last+sLast);
            }
            if(i>1)temp.add(1);
            res.add(temp);
        }
        return res;
    }
}
